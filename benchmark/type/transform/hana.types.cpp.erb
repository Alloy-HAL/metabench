#include <boost/hana/experimental/types.hpp>
#include <boost/hana/transform.hpp>
#include <boost/hana/type.hpp>
namespace hana = boost::hana;

template<typename>
struct f {
    struct type;
};

template<int> struct x;

int main() {
    constexpr auto xs = hana::experimental::types<
        <%= (1..n).map { |i| "x<#{i}>" }.join(', ') %>
    >{};

#if defined(METABENCH)
    constexpr auto result = hana::transform(xs, hana::metafunction<f>);
#endif
}
