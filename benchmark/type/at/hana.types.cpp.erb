#include <boost/hana/at.hpp>
#include <boost/hana/experimental/types.hpp>
namespace hana = boost::hana;


template <int> struct x;


int main() {
    constexpr auto xs = hana::experimental::types<
        <%= (1..n).map { |i| "x<#{i}>" }.join(', ') %>
    >{};

    constexpr auto result = hana::at_c<<%= n - 1 %>>(xs);
    (void)result;
}
