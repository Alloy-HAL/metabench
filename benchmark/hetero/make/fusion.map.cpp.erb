#include <boost/fusion/include/make_map.hpp>
namespace fusion = boost::fusion;


template <int i>
struct x { };

struct undefined { };

int main() {
#if defined(METABENCH)
    auto map = fusion::make_map<<%=
        (1..n).map { |i| "x<#{i}>" }.join(', ')
    %>>(<%=
        (1..n).map { |i| "undefined{}" }.join(', ')
    %>);
#endif
}
